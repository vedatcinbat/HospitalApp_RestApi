@model IEnumerable<HospitalWebApi.Models.Patient>
@section style {
    <style>
        table, tr, td, th {
            border: 1px solid black;
            padding: 5px;
        }
    </style>
}


<div class="text-center">
    <div class="text-xl">This is homepage</div>
    <div>Here is the patients table</div>
    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Surname</th>
                <th>From</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var patient in Model)
            {
                <tr>
                    <td>@patient.Id</td>
                    <td>@patient.patientName</td>
                    <td>@patient.patientSurname</td>
                    <td>@patient.patientFrom</td>
                </tr>
            }
        </tbody>
    </table>
</div>
